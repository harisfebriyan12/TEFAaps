{
  "expo": {
    "name": "Tefa Apps",
    "slug": "tefa-apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tefa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7C3AED"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.tefaapps"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d4de35c-a708-4cd0-85e2-eba2c9df251b"
      }
    }
  }
}
